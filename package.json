{
  "name": "puppeteer-discord-bot",
  "version": "1.0.0",
  "description": "Bot de Discord con Puppeteer para monitorear y reportar actividades.",
  "main": "los2.cjs",
"scripts": {
    "setup-and-start": "npm run install-chrome && npm run start",
    "start": "node los2.cjs",
    "install-chrome": "puppeteer install --force", // Fuerza la instalaciÃ³n de Chromium
    "postinstall": "puppeteer install --force",
    "debug": "node -e \"console.log(require('puppeteer').executablePath())\"",
    "clean-install": "rm -rf node_modules && npm install"
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "keywords": ["puppeteer", "discord", "bot", "automation"],
  "author": "caca",
  "license": "ISC",
  "dependencies": {
    "discord.js": "14.16.3",
    "dotenv": "10.0.0",
    "puppeteer": "23.10.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
